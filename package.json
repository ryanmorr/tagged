    "@rollup/plugin-terser": "^0.4.0",
    "chai": "^4.3.7",
    "eslint": "^8.33.0",
    "mocha": "^10.2.0",
    "rollup": "^3.14.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "sinon": "^15.0.1",
    "terser": "^5.16.3"
